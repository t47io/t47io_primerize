{
  "name": "primerize-app",
  "version": "2.0.1",
  "description": "Primerize server implemented with react.js",
  "main": "src/index.js",
  "engines": {
    "node": "^8.1.3",
    "yarn": "^0.27.5"
  },
  "scripts": {
    "dev": "./node_modules/nodemon/bin/nodemon.js -e js,jsx,json,css,scss,html -i public/ -x babel-node -d 3 -V server/server.js",
    "prod": "pm2 start --interpreter babel-node server/server.js -n main",

    "build": "BABEL_ENV=production babel-node ./node_modules/webpack/bin/webpack.js --config webpack.config.js --verbose",

    "lint": "./node_modules/eslint/bin/eslint.js applications src webpack.*.js --ext [.jsx,.js] --fix",

    "update": "yarn upgrade-interactive --exact && yarn clean"
  },
  "repository": "github:t47io/t47io_primerize",
  "license": "GPL-3.0",
  "author": "Siqi Tian <contact@t47.io>",
  "homepage": "https://primerize.t47.io/",
  "keywords": [
    "primerize",
    "primer assembly",
    "RNA synthesis",
    "mutagenesis library",
    "t47io"
  ],
  "devDependencies": {
    "autoprefixer": "7.1.2",
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.1.1",
    "babel-plugin-lodash": "3.2.11",
    "babel-plugin-module-resolver": "2.7.1",
    "babel-preset-babili": "0.1.4",
    "babel-preset-env": "1.6.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-2": "6.24.1",
    "bundle-loader": "0.5.5",
    "classnames": "2.2.5",
    "compression": "1.7.0",
    "css-loader": "0.28.4",
    "eslint": "4.3.0",
    "eslint-config-airbnb": "15.1.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.1.0",
    "express-static-gzip": "0.3.0",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.2",
    "html-minifier": "3.5.3",
    "html-webpack-plugin": "2.29.0",
    "lodash-webpack-plugin": "0.11.4",
    "material-ui": "1.0.0-beta.2",
    "material-ui-icons": "1.0.0-alpha.19",
    "node-sass": "4.13.1",
    "nodemon": "1.11.0",
    "null-loader": "0.1.1",
    "postcss": "6.0.8",
    "postcss-loader": "2.0.6",
    "prop-types": "15.5.10",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "react-redux": "5.0.5",
    "react-router-dom": "4.1.2",
    "redux": "3.7.2",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.2.0",
    "resolve-url-loader": "2.1.0",
    "sass-loader": "6.0.6",
    "style-loader": "0.18.2",
    "typeface-roboto": "0.0.31",
    "uglify-js": "2.8.29",
    "uglifyjs-webpack-plugin": "0.4.6",
    "url-loader": "0.5.9",
