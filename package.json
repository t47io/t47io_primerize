    "helmet": "3.8.0",
    "pm2": "2.6.1",
    "sanitizer": "0.1.3",
    "serve-favicon": "2.4.3",
    "shelljs": "0.7.8"
  },
  "os": [
    "darwin",
    "linux"
  ],
  "preferGlobal": false,
  "private": true
}
